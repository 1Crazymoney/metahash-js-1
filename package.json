{
  "name": "metahash-js",
  "version": "0.2.0",
  "description": "JS SDK for MetaHash Blockchain",
  "module": "src/index",
  "main": "src/index.js",
  "unpkg": "dist/metahash.min.js",
  "browser": "dist/metahash.min.js",
  "files": [
    "/src/",
    "/dist/"
  ],
  "license": "MIT",
  "homepage": "https://github.com/xboston/metahash-js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xboston/metahash-js.git"
  },
  "keywords": [
    "metahash",
    "blockchain",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/xboston/metahash-js/issues"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "dohdec": "2.0.0",
    "elliptic": "6.5.0",
    "key-encoder": "2.0.2",
    "locutus": "2.0.11",
    "node-fetch": "2.6.0",
    "extend": "^2.0.2"
  },
  "devDependencies": {
    "browserify": "16.5.0",
    "tinyify": "2.5.1"
  },
  "scripts": {
    "build": "browserify --standalone MetaHash src/index.js > dist/metahash.js",
    "build:min": "browserify -p tinyify --standalone MetaHash src/index.js > dist/metahash.min.js",
    "build:full": "npm run build && npm run build:min"
  }
}